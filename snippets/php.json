{
	"Public Method": {
		"prefix": "met",
		"body": [
			"public function $1($2)\r",
			"{\r",
			"    $3\r",
			"}"
		],
		"description": "New Public Method"
	},
	"Protected Function": {
		"prefix": "pmet",
		"body": [
			"protected function $1($2)\r",
			"{\r",
			"    $3\r",
			"}"
		],
		"description": "New Protected Function"
	},
	"Protected Guarded": {
		"prefix": "pguarded",
		"body": [
			"protected $guarded = [];"
		],
		"description": "Creates a protected $guarded empty array"
	},
	"Public Function __construct": {
		"prefix": "__met",
		"body": [
			"public function __construct($1)\r",
			"{\r",
			"    $2\r",
			"}"
		],
		"description": "Create new public function __construct method"
	},
	"Public Static Method": {
		"prefix": "smet",
		"body": [
			"public static function $1()\r",
			"{\r",
			"    $2\r",
			"}"
		],
		"description": "Create new public static method"
	},
	"var_dump": {
		"prefix": "_dump",
		"body": [
			"echo '<pre>';\r",
			"var_dump($1);\r",
			"echo '</pre>';"
		],
		"description": "Pre selected text view of var_dump function"
	},
	"Private Method": {
		"prefix": "vmet",
		"body": [
			"private function $1($2)",
			"{\r",
			"\t$3\r",
			"}"
		],
		"description": "New Private Method"
	},
	"Test Function": {
		"prefix": "@test",
		"body": [
			"/** @test */\r",
			"function it_$1()\r",
			"{\r",
			"\t$2\r",
			"}"
		],
		"description": "Create a test function."
	},
	"PHP Class": {
		"prefix": "phpcl",
		"body": [
			"namespace $1;\r",
			"\r",
			"class $2\r",
			"{\r",
			"\t$3\r",
			"}"
		],
		"description": "Create a PHP class."
	}
}
